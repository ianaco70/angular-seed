{
  "name": "angular-seed",
  "version": "1.0.1",
  "description": "angular application template",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "test": "./node_modules/.bin/karma start --singleRun true",
    "vendorBuild": "./node_modules/.bin/webpack --config ./src/vendor/webpack.config.js",
    "prebuild": "npm install && npm run clean && npm run vendorBuild",
    "build": "./node_modules/.bin/webpack --env production",
    "prestart": "npm run clean && npm run vendorBuild",
    "start": "./node_modules/.bin/webpack-dev-server --env development"
  },
  "keywords": [
    "angular"
  ],
  "author": "jianacone",
  "license": "MIT",
